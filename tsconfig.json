{
    "compilerOptions": {
      "sourceMap": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": false,
      "lib": ["esnext"],
      "esModuleInterop": true,
      "declaration": true,
      "baseUrl": "./src",
      "paths": {
        "@handlers/*": [
          "handlers/*"
        ],
        "@type/*": [
          "types/*"
        ],
        "@modules/*": [
          "modules/*"
        ],
        "@routes/*": [
          "routes/*"
        ],
        "@middlewares/*": [
          "middlewares/*"
        ],
        "@schemas/*": [
          "schemas/*"
        ],
        "@utils/*": [
          "utils/*"
        ],
        "@db/*": [
          "db"
        ]
      }
    },
    "include": ["index.d.ts, global.d.ts", "src/**/*.ts", ]
  }